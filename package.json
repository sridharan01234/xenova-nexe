{
  "name": "context-provider",
  "version": "1.0.0",
  "description": "A Node.js CLI application that converts workspaces into vector embeddings using Xenova/Transformers",
  "main": "src/index.js",
  "bin": {
    "context-provider": "./src/index.js"
  },
  "scripts": {
    "start": "node src/index.js",
    "dev": "node src/index.js",
    "build": "node build.js",
    "build-wrapper": "node build-wrapper.js",
    "test": "node src/index.js --help"
  },
  "keywords": [
    "cli",
    "vector-embeddings",
    "transformers",
    "workspace",
    "nexe"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@xenova/transformers": "^2.17.2",
    "commander": "^11.1.0",
    "fs-extra": "^11.2.0",
    "glob": "^8.1.0",
    "ignore": "^5.3.0",
    "mime-types": "^2.1.35",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "nexe": "^5.0.0-beta.4"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "nexe": {
    "target": "linux-x64-20.11.1",
    "build": true,
    "output": "dist/context-provider",
    "temp": "./.nexe-temp",
    "resources": [
      "package.json"
    ]
  }
}
